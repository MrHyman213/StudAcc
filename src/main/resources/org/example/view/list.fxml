<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="343.0" prefWidth="390.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.ListController">
	<children>
		<Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#addAct" text="Добавить" />
		<Button layoutX="95.0" layoutY="14.0" mnemonicParsing="false" onAction="#changeAct" text="Редактировать" />
		<Button layoutX="209.0" layoutY="14.0" mnemonicParsing="false" onAction="#deleteAct" text="Удалить" />
		<ListView fx:id="itemList" layoutX="13.0" layoutY="94.0" prefHeight="198.0" prefWidth="359.0" />
		<Button layoutX="307.0" layoutY="302.0" mnemonicParsing="false" onAction="#exitAct" text="Выход" />
		<TextField fx:id="tfSelectedItems" layoutX="12.0" layoutY="55.0" prefHeight="31.0" prefWidth="359.0" promptText="Введите название элемента, который хотите добавить" />
	</children>
</AnchorPane>
