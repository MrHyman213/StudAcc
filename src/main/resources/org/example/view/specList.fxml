<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.SpecializationController">
   <children>
      <ListView fx:id="specList" layoutX="14.0" layoutY="94.0" prefHeight="412.0" prefWidth="772.0" />
      <Label layoutX="510.0" layoutY="16.0" text="Список специальностей">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <ComboBox fx:id="cbLists" layoutX="14.0" layoutY="21.0" prefWidth="150.0" />
      <Button layoutX="173.0" layoutY="21.0" mnemonicParsing="false" onAction="#moveAct" text="Перевести ⏩" />
      <Button layoutX="267.0" layoutY="21.0" mnemonicParsing="false" onAction="#exitAct" text="Выход 🏠" />
      <Button layoutX="14.0" layoutY="55.0" mnemonicParsing="false" onAction="#updateList" text="🔄">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="btAdmin" layoutX="57.0" layoutY="55.0" mnemonicParsing="false" onAction="#adminPanelAct" text="Админ-панель 🌐">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="lbLists" layoutX="25.0" layoutY="25.0" text="Справочники 📋" />
   </children>
</AnchorPane>
